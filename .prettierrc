{
    "singleQuote": true,
    "printWidth": 120,
    "semi": true,
    "trailingComma": "none",
    "jsxBracketSameLine": false,
    "useTabs": true,
    "tabWidth": 2,
    "bracketSpacing": true,
    "arrowParens": "always",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "useTabs": false,
                "tabWidth": 2
            }
        }
    ]
}
